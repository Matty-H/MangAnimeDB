{
  "name": "manganimedb",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "prisma": {
    "schema": "./backend/prisma/schema.prisma"
  },
  "scripts": {
    "dev:client": "vite",
    "dev:net": "vite --host",
    "dev:server": "node backend/apiServer.js",
    "dev:db": "npx prisma studio --schema=backend/prisma/schema.prisma",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "net": "concurrently \"npm run dev:net\" \"npm run dev:server\"",
    "fulldev": "concurrently \"npm run dev:client\" \"npm run dev:server\" \"npm run dev:db\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "debug:routes": "node -e \"console.log(require('@tanstack/router-generator').getConfig())\"",
    "generate-routes": "tsr generate",
    "watch-routes": "tsr watch",
    "build-routes": "npm run generate-routes && ...",
    "dev-routes": "npm run watch-routes && ...",
    "prismagen": "prisma generate --schema=backend/prisma/schema.prisma",
    "seed": "ts-node backend/prisma/seed.ts",
    "ping": "npx ts-node test-db.ts",
    "format": "prettier --write .",
    "add-comment": "node add-comment.js",
    "dbreboot": "npx prisma migrate reset && npx prisma migrate dev --name init && npm run seed"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.30.4",
    "@clerk/express": "^1.4.16",
    "@prisma/client": "^6.6.0",
    "@tailwindcss/vite": "^4.1.5",
    "@tanstack/react-form": "^1.9.0",
    "@tanstack/react-router": "^1.116.0",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "daisyui": "^5.0.35",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "lucide-react": "^0.507.0",
    "next": "^15.3.0",
    "pg": "^8.14.1",
    "prisma": "^6.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.2",
    "tailwindcss": "^4.1.5",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tanstack/react-router-devtools": "^1.115.2",
    "@tanstack/router-cli": "^1.116.0",
    "@tanstack/router-plugin": "^1.116.0",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
